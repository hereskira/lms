<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Grades</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1>Welcome, <%= username %>!</h1>
    <h2>Your Grades:</h2>
    <table>
        <thead>
            <tr>
                <th>Course Name</th>
                <th>Grade</th>
            </tr>
        </thead>
        <tbody>
            <% grades.forEach(grade => { %>
                <tr>
                    <td><%= grade.course_name %></td>
                    <td><%= grade.grade %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
